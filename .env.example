# Database Configuration (PostgreSQL - if not using Supabase)
DATABASE_URL=postgresql://username:password@localhost:5432/database_name

# Supabase Configuration (Backend)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your-service-role-key-here
SUPABASE_ANON_KEY=your-anon-key-here

# Enable/Disable Supabase mode (set to 'true' to use Supabase)
USE_SUPABASE=false

# Frontend Environment Variables (create frontend/.env with these)
REACT_APP_SUPABASE_URL=https://your-project.supabase.co
REACT_APP_SUPABASE_ANON_KEY=your-anon-key-here
REACT_APP_USE_SUPABASE=false
REACT_APP_API_URL=http://localhost:3001/api

# AI Services
ANTHROPIC_API_KEY=your-anthropic-key
OPENAI_API_KEY=your-openai-key

# Processing Configuration
PROCESSING_TIMEOUT=900000

# Node Environment
NODE_ENV=development